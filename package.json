{
  "name": "room-booking-microservices",
  "version": "1.0.0",
  "description": "Room booking system with microservices architecture",
  "scripts": {
    "docker:up": "docker compose --env-file .env.default up -d --build --force-recreate",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down -v",
    "docker:logs": "docker compose logs -f",
    "eslint": "eslint \"be_ms/**/*.js\"",
    "eslint:fix": "eslint --fix \"be_ms/**/*.js\""

  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0"
  }
}
